{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "docker:build": "docker build -t arpmanutencoes-front .",
    "docker:run": "docker run -p 3000:3000 arpmanutencoes-front",
    "docker:dev": "docker-compose --profile dev up --build",
    "docker:prod": "docker-compose --profile production up --build -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker system prune -f && docker volume prune -f",
    "docker:setup": "cp env.docker .env && echo '✅ Arquivo .env criado! Edite com suas configurações.'",
    "deploy:vps": "chmod +x deploy.sh && ./deploy.sh production",
    "deploy:dev": "chmod +x deploy.sh && ./deploy.sh development",
    "deploy:monitor": "chmod +x deploy.sh && ./deploy.sh monitor",
    "deploy:backup": "chmod +x deploy.sh && ./deploy.sh backup",
    "deploy:quick": "chmod +x quick-deploy.sh && ./quick-deploy.sh",
    "deploy:vps-simple": "chmod +x deploy-vps.sh && ./deploy-vps.sh",
    "diagnose": "chmod +x diagnose.sh && ./diagnose.sh",
    "fix-connection": "chmod +x fix-connection.sh && ./fix-connection.sh",
    "fix-ssl": "chmod +x fix-ssl.sh && ./fix-ssl.sh",
    "setup-vps-ip": "chmod +x setup-177-153-51-81.sh && ./setup-177-153-51-81.sh",
    "deploy-ip": "chmod +x deploy-ip-specific.sh && ./deploy-ip-specific.sh",
    "fix-network": "chmod +x fix-network.sh && ./fix-network.sh",
    "diagnose-connectivity": "chmod +x diagnose-connectivity.sh && ./diagnose-connectivity.sh",
    "fix-app-connectivity": "chmod +x fix-app-connectivity.sh && ./fix-app-connectivity.sh"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
